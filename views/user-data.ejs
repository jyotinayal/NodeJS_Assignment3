<!DOCTYPE html>
<html lang="en">
<head>
  <title>Fetch </title>
</head>
<body>
    <div class="table-data">
<h2>Display Userinformation </h2>
    <table border="1">
        <tr>
            
            <th>First Name</th>
            <th>Last Name</th>
            <th>Username</th>
        </tr>
        
        <%
        
        if(userData.length!=0){
            
        userData.forEach(function(allDetails){
            
        %>
        <tr>
            <td> <%=allDetails.firstname %> </td>
            <td><%=allDetails.firstname %></td>
            <td><%=allDetails.lastname %></td>
            <td><%=allDetails.username %></td>
        </tr>
        <%   }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>